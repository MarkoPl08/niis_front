<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.ListView?>
<VBox xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.frontend.niis_front.controllers.UpdateRestaurantController">
    <ListView fx:id="restaurantListView" />
    <TextField fx:id="nameField" promptText="Name" />
    <TextField fx:id="priceField" promptText="ZipCode" />
    <TextField fx:id="codeField" promptText="Location" />
    <Button fx:id="updateButton" text="Update Restaurant" onAction="#updateSelectedRestaurant"/>
</VBox>
